<rdf:RDF xmlns="http://envi.uef.fi/wavellite/wto#"
     xml:base="http://envi.uef.fi/wavellite/wto"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#">
    <owl:Ontology rdf:about="http://envi.uef.fi/wavellite/wto">
        <rdfs:comment>The Wavellite Task Ontology (WTO) is an ontology for the Wavellite entities relevant to the task of situational knowledge acquisition and representation from sensor data for environmental phenomena.</rdfs:comment>
        <owl:imports rdf:resource="http://purl.oclc.org/NET/ssnx/ssn"/>
        <owl:imports rdf:resource="http://purl.org/linked-data/cube"/>
        <owl:imports rdf:resource="http://vistology.com/ont/2008/STO/STO.owl"/>
    </owl:Ontology>
    


